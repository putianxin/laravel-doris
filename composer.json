{
    "name": "wukongdontskipschool/laravel-doris",
    "description": "This laravel extension adds support for doris and starrocks to the query builder and eloquent.",
    "keywords": ["laravel-doris", "doris-orm", "laravel-starrocks", "starrocks-orm"],
    "homepage": "https://github.com/wukongdontskipschool/laravel-doris",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wukongdontskipschool\\LaravelDoris\\": "src/"
        }
    },
    "authors": [
        {
            "name": "wukongdontskipschool"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/database": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wukongdontskipschool\\LaravelDoris\\DatabaseServiceProvider"
            ]
        }
    }
}
